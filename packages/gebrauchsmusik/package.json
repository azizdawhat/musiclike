{
  "author": "Aziz Da <azizdawhat@gmail.com>",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.23.9"
  },
  "description": "German term, meaning \"utility music\".",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "license": "GPL-3.0-or-later",
  "main": "./dist/index.cjs",
  "name": "gebrauchsmusik",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "./packages/gebrauchsmusik",
    "type": "git",
    "url": "https://github.com/azizdawhat/musiclike.git"
  },
  "scripts": {
    "build": "concurrently --raw \"rollup --config \\\"./rollup.config.js\\\"\" \"tsc --project \\\"./tsconfig.json\\\"\"",
    "prebuild": "del --verbose \"./dist/*\"",
    "test": "mocha --config \"./.mocharc.jsonc\""
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.1.0"
}
